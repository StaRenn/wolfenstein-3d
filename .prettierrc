{
  "importOrder": [
    "^.*/view.*",
    "^.*/entities.*",
    "^.*/services.*",
    "^.*/controllers.*",
    "^.*/constants.*",
    "^.*/utils.*",
    "^(?!.*types).*$",
    ".*"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "experimentalBabelParserPluginsList": ["classProperties", "jsx", "decorators-legacy"],
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "es5"
}
